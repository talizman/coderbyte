def ExOh(str)
  charArr = str.split("")
  xCount = 0
  oCount = 0
  charArr.each do |char|
    xCount += 1 if char == "x"
    oCount += 1 if char == "o"
  end
  xCount == oCount ? "true" : "false"
end
   
# keep this function call here 
# to see how to enter arguments in Ruby scroll down   
ExOh(STDIN.gets) 
