def ABCheck(str)
  charArr = str.split("")
  return "false" if charArr.length <= 3

  charArr.each_with_index do |char, index|
    if char == "a" 
      if charArr[index + 4] == "b"
        return "true"
      end
    end
  end
  return "false"
end
   
# keep this function call here 
# to see how to enter arguments in Ruby scroll down   
ABCheck(STDIN.gets)  
